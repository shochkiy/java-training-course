package Main;

public class Main {
    public static void main(String[] args) {
        int[] mass = new int[5];
        for (int i = 0; i < 6; i++) {
            try {
                mass[i] = i;
            } catch (IndexOutOfBoundsException e) {
                System.out.println("Out of bounds mass!");;
            }
        }
        for (int n : mass) {
            System.out.println(n);
        }
    }
}